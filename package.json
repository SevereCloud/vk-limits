{
  "name": "vk-limits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint && stylelint \"**/*.css\"",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vkontakte/icons": "^2.82.0",
    "@vkontakte/vk-bridge": "^2.11.2",
    "@vkontakte/vkui": "^5.9.5",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  }
}
