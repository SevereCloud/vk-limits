{
  "name": "vk-limits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "lint": "next lint && stylelint \"**/*.css\"",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vkontakte/icons": "^2.3.0",
    "@vkontakte/vk-bridge": "^2.7.2",
    "@vkontakte/vkui": "^5.0.3",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.10",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  }
}
