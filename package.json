{
  "name": "vk-limits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "lint": "next lint && stylelint \"**/*.css\"",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vkontakte/icons": "^2.22.0",
    "@vkontakte/vk-bridge": "^2.7.2",
    "@vkontakte/vkui": "^5.2.3",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.5",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^31.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "5.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  }
}
