{
  "name": "vk-limits",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint && stylelint \"**/*.css\"",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vkontakte/icons": "^2.87.0",
    "@vkontakte/vk-bridge": "^2.12.2",
    "@vkontakte/vk-bridge-react": "^1.0.1",
    "@vkontakte/vkui": "^6.0.0-beta.1",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  }
}
